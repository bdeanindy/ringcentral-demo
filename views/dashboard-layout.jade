doctype html
html
  head
    meta(charset='UTF-8')
    title= title
    <!-- Latest compiled and minified CSS -->
    link(href='http://www.ringcentral.com/content/dam/ringcentral/images/favicon.ico' type='image/vnd.microsoft.icon' rel='shortcut icon')
    link(href='http://www.ringcentral.com/content/dam/ringcentral/images/favicon.ico' type='image/vnd.microsoft.icon' rel='icon')
    link(rel='stylesheet', href='/vendor/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/fontawesome-more.css')
    link(rel='stylesheet', href='/vendor/bootstrap/dist/css/bootstrap-theme.min.css')
    link(href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css', rel='stylesheet', integrity='sha256-k2/8zcNbxVIh5mnQ52A0r3a6jAgMGxFJFE2707UxGCk= sha512-ZV9KawG2Legkwp3nAlxLIVFudTauWuBpC10uEafMHYL0Sarrz5A7G79kXh5+5+woxQ5HM559XX2UZjMJ36Wplg==', crossorigin='anonymous')
    link(rel="stylesheet" type="text/css" href="/stylesheets/keen-dashboards.css")
    link(rel="stylesheet" type="text/css" href="/stylesheets/connected-devices.css")
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(type='text/javascript', src='/vendor/jquery/dist/jquery.min.js')
    script(type='text/javascript', src='/vendor/fetch/fetch.js')
    script(type='text/javascript', src='/vendor/es6-promise/promise.js')
    script(type='text/javascript', src='/vendor/pubnub/web/pubnub.js')
    script(type='text/javascript', src='/vendor/ringcentral/build/ringcentral.js')
    script(type='text/javascript', src='/vendor/bootstrap/dist/js/bootstrap.min.js')
    script(type='text/javascript', src='/javascripts/rcDemoApp.js')
    body.application
        .navbar.navbar-inverse.navbar-fixed-top(role='navigation')
          .container-fluid
            .navbar-header
              button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                span.sr-only Toggle navigation
                span.icon-bar
                span.icon-bar
                span.icon-bar
              a.navbar-brand(href='../')
                span.glyphicon.glyphicon-chevron-left
              a.navbar-brand(href='./') Connected Devices
            .navbar-collapse.collapse
              ul.nav.navbar-nav.navbar-left
                li
                  a(href='https://keen.io') Home
                li
                  a(href='https://keen.io/team') Team
                li
                  a(href='https://github.com/keenlabs/dashboards/tree/gh-pages/examples/connected-devices') Source
                li
                  a(href='https://groups.google.com/forum/#!forum/keen-io-devs') Community
                li
                  a(href='http://stackoverflow.com/questions/tagged/keen-io?sort=newest&pageSize=15') Technical Support
        .container-fluid
          .row
            .col-sm-6
              .chart-wrapper
                .chart-title
                  | Traffic Stats
                .chart-stage
                  .row
                    .col-xs-3
                      .tabbable.tabs-left
                        ul.nav.nav-tabs
                          li.active
                            a(href='#visitors', data-toggle='tab') Visitors
                          li
                            a(href='#browser', data-toggle='tab') Browser
                          li
                            a(href='#geography', data-toggle='tab') Geography
                    .col-xs-9
                      .tab-content
                        #visitors.tab-pane.active
                        #browser.tab-pane
                        #geography.tab-pane
            .col-sm-6
              .chart-wrapper
                .chart-title
                  | Stats
                .chart-stage
                  .row
                    .col-sm-6
                      .chart-title.knob-title
                        | New Activations Today
                      .chart-stage
                        input.users(type='text', value='0')
                    .col-sm-6
                      .chart-title.knob-title
                        | Device Crashes Today
                      .chart-stage
                        input.errors(type='text', value='0')
          .row
            .col-sm-6
              .chart-wrapper
                .chart-title
                  | Customer Engagement
                .chart-stage
                  #chart-05
                .chart-notes
            #mapwrap.col-sm-6
              .chart-wrapper
                .chart-title
                  .btn-group-xs
                    | Active Users
                    button#7days.btn.btn-default(type='button') 7 Days
                    button#14days.btn.btn-default(type='button') 14 Days
                    button#28days.btn.btn-default(type='button') 28 Days
                .chart-stage
                  #map
                .chart-notes

      block content

      <!-- Mapbox.js Assets -->
      script(type="text/javascript" src='https://api.tiles.mapbox.com/mapbox.js/v2.0.0/mapbox.js')
      link(href='https://api.tiles.mapbox.com/mapbox.js/v2.0.0/mapbox.css' rel='stylesheet')
      script(type="text/javascript" src='https://api.tiles.mapbox.com/mapbox.js/plugins/leaflet-heat/v0.1.0/leaflet-heat.js')
      script(type="text/javascript" src="/javascripts/jquery.knob.min.js")
      script(type="text/javascript" src="/javascripts/keen.min.js")
      script(type="text/javascript" src="/javascripts/meta.js")
      script(type="text/javascript" src="/javascripts/connected-devices.js")
